{
  "name": "group-tag-bot",
  "version": "1.1.4",
  "repository": {
    "type": "git",
    "url": "https:git@github.com:zp-bots-telegram/group-tag-bot"
  },
  "description": "Telegram Group Tag Bot",
  "main": "src/handler.ts",
  "scripts": {
    "postinstall": "sh -c 'if command -v husky; then is-ci || husky install; fi;'",
    "build": "rimraf dist && webpack --progress --color",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --fix --ext .ts",
    "prettier:format": "prettier --config .prettierrc \"src/**/*.ts\" --write",
    "start": "node dist/bundle.js",
    "start:dev": "nodemon -r dotenv/config --config nodemon.json",
    "test": "yarn test:unit",
    "test:unit": "jest src",
    "test:unit:coverage": "jest src --collectCoverage",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:gen": "prisma generate",
    "db:push": "prisma db push",
    "lint-staged": "lint-staged"
  },
  "keywords": [],
  "author": "Zack Pollard (https://github.com/zp-bots-telegram)",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "express": "^4.18.2",
    "telegraf": "^4.15.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^18.4.3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.3",
    "@types/express": "^4.17.21",
    "@types/node": "^20.9.3",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "commitlint": "^18.4.3",
    "copy-webpack-plugin": "^11.0.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jest": "^29.7.0",
    "lint-staged": "^15.1.0",
    "localtunnel": "^2.0.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "prisma": "^5.6.0",
    "rimraf": "^5.0.5",
    "semantic-release": "^22.0.8",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.3.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "homepage": "https://github.com/zp-bots-telegram/group-tag-bot",
  "volta": {
    "node": "20.9.0",
    "yarn": "4.0.2"
  },
  "packageManager": "yarn@4.0.2"
}
