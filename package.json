{
  "name": "group-tag-bot",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https:git@github.com:zp-bots-telegram/group-tag-bot"
  },
  "description": "Telegram Group Tag Bot",
  "main": "src/handler.ts",
  "scripts": {
    "postinstall": "is-ci || husky install",
    "build": "webpack --progress --color",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --fix --ext .ts",
    "prettier:format": "prettier --config .prettierrc \"src/**/*.ts\" --write",
    "start": "node dist/bundle.js",
    "start:dev": "nodemon -r dotenv/config --config nodemon.json",
    "test": "yarn test:unit",
    "test:unit": "jest src",
    "test:unit:coverage": "jest src --collectCoverage",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:gen": "prisma generate",
    "db:push": "prisma db push",
    "lint-staged": "lint-staged"
  },
  "keywords": [],
  "author": "Zack Pollard (https://github.com/zp-bots-telegram)",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^3.9.2",
    "express": "^4.17.2",
    "telegraf": "^4.7.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^16.2.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.1",
    "@types/express": "^4.17.13",
    "@types/node": "^15.0.1",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "commitlint": "^16.2.1",
    "copy-webpack-plugin": "^10.2.4",
    "dotenv": "^16.0.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.4",
    "localtunnel": "^2.0.2",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "prisma": "^3.9.2",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.4.2",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "ts-node": "^9.1.1",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.2.4",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2"
  },
  "homepage": "https://github.com/zp-bots-telegram/group-tag-bot"
}
